{
	"compilerOptions": {
		"jsx": "react-native",
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"moduleResolution": "Node",
		"target": "ESNext",
		"module": "ESNext",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"lib": [
			"ESNext",
			"DOM"
		]
	}
}